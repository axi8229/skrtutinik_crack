<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/visit_item_0"
    android:background="?attr/selectableItemBackground"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/padding_default"
    android:paddingEnd="@dimen/padding_default">
    <ImageView
        android:id="@+id/iv_visitor_item"
        android:tag="binding_1"
        android:padding="5dp"
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textSize="@dimen/text_main"
        android:textColor="@color/large_text"
        android:ellipsize="end"
        android:id="@+id/tv_time_visit_item"
        android:tag="binding_2"
        android:paddingTop="10dp"
        android:paddingBottom="0dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        app:layout_constraintStart_toEndOf="@+id/iv_visitor_item"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/TextView.Body2"/>
    <TextView
        android:textSize="@dimen/text_small"
        android:textColor="@color/sputnik_black"
        android:ellipsize="end"
        android:id="@+id/tv_adress_visit_item"
        android:tag="binding_3"
        android:paddingTop="5dp"
        android:paddingBottom="0dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_visitor_item"
        app:layout_constraintEnd_toStartOf="@+id/iv_archive"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@+id/iv_visitor_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_time_visit_item"
        style="@style/TextView.Body2"/>
    <ImageView
        android:id="@+id/iv_phone_call"
        android:tag="binding_4"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/ic_phone_off"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_time_visit_item"
        app:layout_constraintStart_toEndOf="@+id/tv_time_visit_item"/>
    <ImageView
        android:id="@+id/iv_archive"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/ic_archive_outline_new"
        android:paddingStart="0dp"
        android:paddingEnd="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/divider_h"
        android:background="#ededed"
        android:layout_width="0dp"
        android:layout_marginTop="@dimen/margin_default"
        android:layout_marginStart="@dimen/margin_default"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_adress_visit_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_adress_visit_item"
        style="@style/main_divider_style"/>
</androidx.constraintlayout.widget.ConstraintLayout>
