<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cardview="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_root"
    android:tag="layout/item_market_key_0"
    android:background="@drawable/background_card"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="6dp"
    android:layout_marginBottom="6dp"
    android:layout_marginStart="20dp"
    android:layout_marginEnd="20dp"
    android:backgroundTint="#f4f4f4"
    cardview:cardCornerRadius="12dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        cardview:layout_constraintBottom_toBottomOf="parent"
        cardview:layout_constraintEnd_toEndOf="parent"
        cardview:layout_constraintStart_toStartOf="parent"
        cardview:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="@dimen/text_title_2"
            android:textColor="@color/sputnik_header_black"
            android:id="@+id/tv_title"
            android:paddingLeft="0dp"
            android:paddingTop="26dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/play_bold"
            android:layout_marginStart="@dimen/margin_start_end"
            android:layout_marginEnd="@dimen/margin_default"
            android:paddingHorizontal="0dp"
            cardview:layout_constraintBottom_toTopOf="@+id/serviceDescription"
            cardview:layout_constraintEnd_toStartOf="@+id/imageView23"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toTopOf="parent"
            style="@style/TextView.Header2"/>
        <TextView
            android:textSize="@dimen/text_small"
            android:textColor="@color/track_gray"
            android:id="@+id/tv_subtitle"
            android:padding="0dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_default"
            android:layout_marginStart="@dimen/margin_start_end"
            android:layout_marginEnd="@dimen/margin_small"
            cardview:layout_constraintEnd_toStartOf="@+id/imageView23"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/tv_title"
            style="@style/TextView.Body1"/>
        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="181dp"
            android:layout_height="234dp"
            android:src="@drawable/ic_market_key"
            android:scaleType="fitEnd"
            cardview:layout_constraintEnd_toEndOf="parent"
            cardview:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="@dimen/text_main"
            android:textColor="@color/sputnik_blue"
            android:id="@+id/tv_open_catalog"
            android:padding="0dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_default_plus"
            android:layout_marginBottom="@dimen/margin_medium"
            android:layout_marginStart="@dimen/margin_medium"
            android:layout_marginEnd="@dimen/margin_default"
            cardview:layout_constraintBottom_toBottomOf="parent"
            cardview:layout_constraintEnd_toStartOf="@+id/imageView23"
            cardview:layout_constraintStart_toStartOf="parent"
            cardview:layout_constraintTop_toBottomOf="@+id/tv_subtitle"
            style="@style/TextView.Body2"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
