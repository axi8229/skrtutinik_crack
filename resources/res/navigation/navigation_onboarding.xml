<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/onboarding_navigation"
    app:startDestination="@+id/onboardingFragment">
    <fragment
        android:name="com.sputnik.oboarding.ui.onboarding.OnBoardingFragment"
        android:id="@+id/onboardingFragment">
        <action
            android:id="@+id/action_onboardingFragment_to_new_registration_navigation"
            app:destination="@+id/new_registration_navigation"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_exit_anim"
            app:popUpTo="@+id/onboarding_navigation"
            app:popUpToInclusive="true"/>
    </fragment>
    <include app:graph="@navigation/navigation_new_registration"/>
</navigation>
